<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.springboot.example.springbootssm.mapper.EmployeeMapper">

    <select id="getEmpById" resultType="com.springboot.example.springbootssm.pojo.Employee">
        select * from mybatis_emp where emp_id=#{id}
    </select>
</mapper>